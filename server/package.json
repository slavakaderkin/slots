{
  "name": "server",
  "version": "0.0.1",
  "main": "server.js",
  "types": "types/global.d.ts",
  "scripts": {
    "test": "npm run lint && npm run types && MODE=test node server.js",
    "dotest": "npm run lint && npm run types && node server.js",
    "types": "tsc -p tsconfig.json",
    "lint": "eslint . && prettier -c \"**/*.js\" \"**/*.json\" \"**/*.md\"",
    "fix": "eslint . --fix && prettier --write \"**/*.js\" \"**/*.json\" \"**/*.md\"",
    "db": "metasql c"
  },
  "engines": {
    "node": "18 || 20 || 21 || 22 || 23 || 24"
  },
  "devDependencies": {
    "@types/node": "^22.15.30",
    "@types/pg": "^8.15.4",
    "@types/ws": "^8.18.1",
    "eslint": "^9.28.0",
    "eslint-config-metarhia": "^9.1.2",
    "prettier": "^3.3.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@metarhia/sql": "^0.2.11",
    "impress": "^3.0.18",
    "metasql": "^3.0.0-alpha.3",
    "pg": "^8.16.0",
    "redis": "^5.5.6"
  }
}
